<% @resource.as_json.each do |attr, val| %>
    "<%= attr %>": <%= val.to_json.html_safe %>,
<% end %>

"auth_token": "<%= @token %>",
"message":    "deliverCredentials",
"client_id":  "<%= @client_id %>",
"expiry":     "<%= @expiry %>",
<% if @oauth_registration %>
"oauth_registration": "true",
<% end %>
"config":     "<%= @config %>"